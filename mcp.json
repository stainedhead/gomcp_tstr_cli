{
  "servers": {
    "filesystem": {
      "name": "filesystem",
      "command": ["mcp-server-filesystem"],
      "args": ["/tmp"],
      "transport": {
        "type": "stdio"
      },
      "env": {
        "PATH": "/usr/local/bin:/usr/bin:/bin"
      }
    },
    "example_http": {
      "name": "example_http",
      "transport": {
        "type": "http",
        "host": "localhost",
        "port": 8080,
        "path": "/mcp"
      }
    },
    "example_sse": {
      "name": "example_sse",
      "transport": {
        "type": "sse",
        "host": "localhost",
        "port": 9090,
        "path": "/events"
      }
    }
  }
}
